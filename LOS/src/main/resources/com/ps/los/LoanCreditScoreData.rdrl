package com.ps.los;

import java.lang.Number;

rule "LoanCreditScoreData"
	dialect "mvel"
	ruleflow-group "LoanCreditScoring"
	when
		customerDetails : CustomerDetails( gender == "Male" , gender == "Female" , gender == null )
	then
		CreditScoringData creditScoring = new CreditScoringData();
		creditScoring.setGender_creditScore( 10 );
		creditScoring.setGender_creditScore( 5 );
		creditScoring.setGender_creditScore( 0 );
		insertLogical( creditScoring );
end
