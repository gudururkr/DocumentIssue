package com.ps.los;

import java.lang.Number;

rule "LoanCreditScoreData"
	dialect "mvel"
	ruleflow-group "LoanCreditScoring"
	when
		CustomerDetails( gender == "Male" , gender == "Female" , gender == null )
	then
		CreditScoringData fact0 = new CreditScoringData();
		fact0.setAge_creditScore( 10 );
		fact0.setAge_creditScore( 5 );
		fact0.setAge_creditScore( 0 );
		insert( fact0 );
end
